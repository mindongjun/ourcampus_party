$(function() {
  $('#univ-ranking').vTicker();
  $('#celeb-ranking').vTicker();
  
  <% for i in 1..6 do %>
    $("#search-ranking<%= i %>").click(function(){
      var value = $("#search-value<%= i%>").text();
      
      $("#total_search").val(value);
      $("#total_search_form").submit();
    });
    
  <% end %>
  
});
